{
  "name": "xolid",
  "description": "A declarative JavaScript library for building user interfaces.",
  "version": "0.6.9",
  "author": "Ryan Carniato",
  "license": "MIT",
  "homepage": "https://solidjs.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/solidjs/solid"
  },
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "browser": {
    "./dist/server.cjs": "./dist/solid.cjs",
    "./dist/server.js": "./dist/solid.js"
  },
  "unpkg": "./src/index.ts",
  "types": "./src/index.ts",
  "sideEffects": false,
  "type": "module",
  "files": [
    "src",
    "src/reactive",
    "src/render",
    "src/server",
    "src/jsx-runtime",
    "dist",
    "store/dist",
    "store/src",
    "store/package.json",
    "web/dist",
    "web/src",
    "web/package.json",
    "universal/dist",
    "universal/src",
    "universal/package.json",
    "src/jsx-runtime/index.ts"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./src/index.ts",
        "default": "./dist/solid.js"
      },
      "require": "./dist/solid.cjs"
    },
    "./store": {
      "import": {
        "types": "./store/src/index.ts",
        "default": "./store/dist/store.js"
      },
      "require": "./store/dist/store.cjs"
    },
    "./web": {
      "import": {
        "types": "./web/src/index.ts",
        "default": "./web/src/index.ts"
      },
      "require": "./web/dist/web.cjs"
    }
  },
  "scripts": {
    "build": "npm-run-all -nl build:*",
    "build:clean": "rimraf dist/ coverage/ store/dist/ web/dist/",
    "build:js": "rollup -c",
    "types": "npm-run-all -nl types:*",
    "types:clean": "rimraf types/ store/types/ web/types/",
    "types:copy": "ncp ../../node_modules/dom-expressions/src/jsx.d.ts src/jsx-runtime/index.ts",
    "types:src": "tsc --noEmit --project ./tsconfig.json",
    "types:web": "tsc --noEmit --project ./web/tsconfig.json",
    "types:store": "tsc --noEmit --project ./store/tsconfig.json",
    "types:universal": "tsc --noEmit --project ./universal/tsconfig.json",
    "bench": "node --allow-natives-syntax bench/bench.cjs",
    "link": "symlink-dir . node_modules/xolid",
    "test": "jest",
    "coverage": "jest --coverage",
    "test-types": "tsc --noEmit --project tsconfig.test.json"
  },
  "keywords": [
    "solid",
    "solidjs",
    "ui",
    "reactive",
    "components",
    "compiler",
    "performance"
  ],
  "dependencies": {
    "csstype": "^3.1.0"
  },
  "devDependencies": {
    "babel-plugin-transform-rename-import": "^2.3.0"
  }
}
