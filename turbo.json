{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputMode": "new-only"
    },
    "xolid#build": {
      "outputs": ["dist/**", "**/dist/**"]
    },
    "xolid#link": {
      "outputs": []
    },
    "xolid#types": {
      "dependsOn": ["^types"],
      "outputs": ["types/**", "**/types/**"]
    },
    "coverage": {
      "outputs": ["coverage/**"]
    },
    "test": {
      "outputs": []
    },
    "test-integration#test": {
      "dependsOn": ["xolid#build", "xolid#link"]
    },
    "xolid#test-types": {
      "outputs": [],
      "dependsOn": ["types"]
    }
  }
}
